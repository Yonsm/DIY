{
	"manifest_version": 3,
	"name": "小米路由器+",
	"version": "1.0",
	"description": "为小米路由器控制台添加设备排序、批量选择、批量导入等功能",
	"permissions": ["activeTab", "scripting"],
	"content_scripts": [
		{
			"matches": ["*://*/*/web/setting/lannetset", "*://*/*/web/home"],
			"js": ["content.js"]
		}
	],
	"icons": {
		"48": "icon.png"
	}
}
